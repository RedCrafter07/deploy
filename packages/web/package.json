		"@fontsource/figtree": "^5.0.5",
		"@swc/core": "^1.3.73",
		"@types/body-parser": "^1.19.2",
		"@types/dockerode": "^3.3.19",
		"@types/express": "^4.17.17",
		"@types/node": "^20.4.5",
		"@types/react": "^18.2.18",
		"@types/react-dom": "^18.2.7",
		"@types/webpack": "^5.28.1",
		"autoprefixer": "^10.4.14",
		"axios": "^1.4.0",
		"body-parser": "^1.20.2",
		"chalk": "4.1.2",
		"css-loader": "^6.8.1",
		"dockerode": "^3.3.5",
		"esbuild": "^0.18.17",
		"express": "^4.18.2",
		"http-terminator": "^3.2.0",
		"postcss": "^8.4.27",

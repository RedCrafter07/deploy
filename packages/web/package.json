{
	"name": "config",
	"version": "1.0.0",
	"description": "The config for RedDeploy",
	"main": "index.js",
	"scripts": {
		"dev": "concurrently \"webpack watch -c webpack.config.ts --mode development\" \"tsx watch --clear-screen=false ./src/server/index.ts\"",
		"build": "webpack -c webpack.config.ts --mode production && pnpm build:server",
		"build:server": "esbuild src/server/index.ts --bundle --platform=node --packages=external --outfile=dist/server/index.js",
		"start": "node dist/server/index.js"
	},
	"keywords": [],
	"author": "RedCrafter07",
	"license": "GPL-3.0",
